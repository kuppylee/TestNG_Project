<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Loan Department">
  <test verbose="2" preserve-order="true" name="Personal Loan">
    <classes>
      <class name="test.Day1Test">
          <methods>
              <exclude name="demo"></exclude>
          </methods>
      </class>
      <class name="test.Day2">
      </class>
    </classes>
  </test>
    <test verbose="2" preserve-order="true" name="Car Loan">
        <classes>
            <class name="test.Day3">
                <methods>
                    <exclude name="mobile.*"></exclude>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true" name="Home Loan">
        <classes>
            <class name="test.Day4">
                <methods>
                    <exclude name="mobile.*"></exclude>
                </methods>
            </class>
        </classes>
    </test>

</suite>
